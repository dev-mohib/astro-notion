---
import Layout from "@layouts/Layout.astro";
import {
        StaticWebsite, FullStack, AWS, BaasPlatform, 
        ContainerService, Terraform, Automation, ArrowCircle, CheckMark, 
        Vue, Vercel, Typescript, Supabase, AstroJs, Mailjet, GoogleCloud, 
        Tailwind, Stripe, Github, Cloudflare, Laravel, Sanity, Saleforce, 
        StoryBlock, Drupal, Wordpress, FirebaseFilled, ReactJs, AppWrite, 
        Strapi, Svelte, GraphQL, ElasticSearch, QuestionMark,NodeJs
} from "@Icons"
